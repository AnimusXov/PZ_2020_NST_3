<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.entities.SupplyEntity" table="supply" schema="public" catalog="Firma">
        <id name="supplyId">
            <column name="supply_id" sql-type="integer"/>
        </id>
        <property name="metallicMaterials">
            <column name="metallic_materials" sql-type="integer"/>
        </property>
        <property name="woodenMaterials">
            <column name="wooden_materials" sql-type="integer"/>
        </property>
        <property name="composites">
            <column name="composites" sql-type="integer"/>
        </property>
        <property name="marble">
            <column name="marble" sql-type="integer"/>
        </property>
        <property name="stoneMaterials">
            <column name="stone_materials" sql-type="integer"/>
        </property>
        <property name="name">
            <column name="name" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>