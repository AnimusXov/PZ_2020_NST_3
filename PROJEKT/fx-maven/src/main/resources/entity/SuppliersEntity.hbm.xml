<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.entities.SuppliersEntity" table="suppliers" schema="public" catalog="Firma">
        <id name="supplierId">
            <column name="supplier_id" sql-type="integer"/>
        </id>
        <property name="companyName">
            <column name="company_name" sql-type="text"/>
        </property>
    </class>
</hibernate-mapping>