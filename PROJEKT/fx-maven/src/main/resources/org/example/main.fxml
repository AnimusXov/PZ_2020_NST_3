<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.MainController">
    <VBox id="vbox" alignment="TOP_CENTER" layoutX="60.0" layoutY="100.0" prefHeight="200.0" prefWidth="180.0" spacing="10.0" BorderPane.alignment="CENTER">
        <children>
            <Button fx:id="userList" mnemonicParsing="false" prefHeight="25.0" prefWidth="217.0" text="Lista Użytkowników" />
            <Separator prefWidth="200.0" />
            <Button fx:id="activeUsers" mnemonicParsing="false" prefHeight="25.0" prefWidth="274.0" text="Przeglądaj Aktywnych" />
            <Separator prefWidth="200.0" />
            <Button fx:id="manageUsers" mnemonicParsing="false" onAction="#handleButtonActionUsers" prefHeight="25.0" prefWidth="203.0" text="Zarządzaj Użytkownkami" />
            <Separator prefWidth="200.0" />
            <Button fx:id="taskList" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="193.0" text="Zadania" />
        </children>
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
        <opaqueInsets>
            <Insets />
        </opaqueInsets>
    </VBox>
</AnchorPane>
